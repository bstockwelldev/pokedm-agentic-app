{
  "buildCommand": "cd server && npm install && cd ../client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "echo 'Skipping root install'",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/agent"
    }
  ],
  "functions": {
    "api/agent.js": {
      "includeFiles": "server/**",
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
